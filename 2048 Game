int main(){
	initwindow(520,520);
	setbkcolor(15);
	cleardevice();
	setcolor(0);
	int a,b,c,d=0,e=10,f;
	int rf,rs;
	char g[20];
	char s11[20],s12[20],s13[20],s14[20],s21[20],s22[20],s23[20],s24[20],s31[20],s32[20],s33[20],s34[20],s41[20],s42[20],s43[20],s44[20];
	int d11=0,d12=0,d13=0,d14=0,d21=0,d22=0,d23=0,d24=0,d31=0,d32=0,d33=0,d34=0,d41=0,d42=0,d43=0,d44=0;
	rectangle(50,50,450,450); //ana kare
	line(150,50,150,450),line(250,50,250,450),line(350,50,350,450); //sutunlar
	line(50,150,450,150),line(50,250,450,250),line(50,350,450,350); //satirlar
	//rf=getsayi();
	rf=43;rs=22;
	if(rf==11&&d11==0){d11=2;}else if(rf==12&&d12==0){d12=2;}else if(rf==13&&d13==0){d13=2;}else if(rf==14&&d14==0){d14=2;}
	else if(rf==21&&d21==0){d21=2;}else if(rf==22&&d22==0){d22=2;}else if(rf==23&&d23==0){d23=2;}else if(rf==24&&d24==0){d24=2;}
	else if(rf==31&&d31==0){d31=2;}else if(rf==32&&d32==0){d32=2;}else if(rf==33&&d33==0){d33=2;}else if(rf==34&&d34==0){d34=2;}
	else if(rf==41&&d41==0){d41=2;}else if(rf==42&&d42==0){d42=2;}else if(rf==43&&d43==0){d43=2;}else if(rf==44&&d44==0){d44=2;}
	//rs=getsayi();
	while(rs==rf){rs=getsayi();};
	if(rs==11&&d11==0){d11=2;};if(rs==12&&d12==0){d12=2;};if(rs==13&&d13==0){d13=2;};if(rs==14&&d14==0){d14=2;};
	if(rs==21&&d21==0){d21=2;};if(rs==22&&d22==0){d22=2;};if(rs==23&&d23==0){d23=2;};if(rs==24&&d24==0){d24=2;};
	if(rs==31&&d31==0){d31=2;};if(rs==32&&d32==0){d32=2;};if(rs==33&&d33==0){d33=2;};if(rs==34&&d34==0){d34=2;};
	if(rs==41&&d41==0){d41=2;};if(rs==42&&d42==0){d42=2;};if(rs==43&&d43==0){d43=2;};if(rs==44&&d44==0){d44=2;};
	sprintf(g,"%d",rs);
	outtextxy(10,10,g);
		sprintf(s11,"%d",d11),sprintf(s12,"%d",d12),sprintf(s13,"%d",d13),sprintf(s14,"%d",d14);
		sprintf(s21,"%d",d21),sprintf(s22,"%d",d22),sprintf(s23,"%d",d23),sprintf(s24,"%d",d24);
		sprintf(s31,"%d",d31),sprintf(s32,"%d",d32),sprintf(s33,"%d",d33),sprintf(s34,"%d",d34);
		sprintf(s41,"%d",d41),sprintf(s42,"%d",d42),sprintf(s43,"%d",d43),sprintf(s44,"%d",d44);
		settextstyle(1,HORIZ_DIR,3);
		outtextxy(100,100,s11),outtextxy(200,100,s12),outtextxy(300,100,s13),outtextxy(400,100,s14);
		outtextxy(100,200,s21),outtextxy(200,200,s22),outtextxy(300,200,s23),outtextxy(400,200,s24);
		outtextxy(100,300,s31),outtextxy(200,300,s32),outtextxy(300,300,s33),outtextxy(400,300,s34);
		outtextxy(100,400,s41),outtextxy(200,400,s42),outtextxy(300,400,s43),outtextxy(400,400,s44);
	while(1){
		a=getch();
		if(a==72){
			//once bosluk varsa aktar *sonradan yazdim*
			if(d31>0&&d21==0&&d11==0){yukari2x(d11,d31,100,300,10,e,15);d11=d31,d31=0;d=1;};if(d31>0&&d21==0&&d11>0){yukarix(d21,d31,100,300,10,e,15);d21=d31,d31=0;d=1;if(d41>0){yukarix(d31,d41,100,400,10,e,15);d31=d41,d41=0;};}
