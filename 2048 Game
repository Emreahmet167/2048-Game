#include <graphics.h>
int getsayi(){
	int a,b,c;
	a=rand();
	b=rand();
	a=a%4;if(a==0){a=4;};
	b=b%4;if(b==0){b=4;};
	c=a*10+b;
	return c;
}
int yukarix(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<100;i+=t1){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*2){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*3){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*4){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*5){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*7){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*8){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*9){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*10){setcolor(r);outtextxy(x1,y1-i+10,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
	}
	sprintf(s,"%d",d1+d2);
	setcolor(r);outtextxy(x1,y1-i+10,s);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);
}
int yukari2x(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<200;i+=t1*2){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*2){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*4){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*8){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*10){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*12){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*14){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*16){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*18){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*20){setcolor(r);outtextxy(x1,y1-i+20,s);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
	}
	sprintf(s,"%d",d1+d2);
	setcolor(r);outtextxy(x1,y1-i+20,s);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);
}
int yukari3x(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<300;i+=t1*3){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*3){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*9){setcolor(r);outtextxy(x1,y1-i+30,s);outtextxy(x1,y1-i-10,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*12){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*15){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*18){setcolor(r);outtextxy(x1,y1-i+30,s);outtextxy(x1,y1-i-10,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*21){setcolor(r);outtextxy(x1,y1-i+30,s);outtextxy(x1,y1-i-10,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*24){setcolor(r);outtextxy(x1,y1-i+30,s);outtextxy(x1,y1-i-10,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*27){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*30){setcolor(r);outtextxy(x1,y1-i+30,s);rectangle(x1,y1-i,x1+25,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
	}
	sprintf(s,"%d",d1+d2);
	setcolor(r);outtextxy(x1,y1-i+30,s);setcolor(0);outtextxy(x1,y1-i,s);rectangle(50,50,450,450);
	line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);
}
int sagx(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<100;i+=t1){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*2){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*3){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*4){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*5){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*7){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*8){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*9){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*10){setcolor(r);outtextxy(x1+i-10,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
	}
	sprintf(s,"%d",d1+d2);
	setcolor(r);outtextxy(x1+i-10,y1,s);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);
}
int sag2x(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<200;i+=t1*2){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*2){setcolor(r);outtextxy(x1+i-20,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*4){setcolor(r);outtextxy(x1+i-20,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1+i-20,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*8){setcolor(r);outtextxy(x1+i-20,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
		line(150,50,150,450),line(250,50,250,450),line(350,50,350,450);line(50,150,450,150),line(50,250,450,250),line(50,350,450,350);delay(t2);break;};
		while(i==t1*10){setcolor(r);outtextxy(x1+i-20,y1,s);rectangle(x1+i,y1,x1-30,y1-15);floodfill(x1+i-1,y1-1,r);setcolor(0);outtextxy(x1+i,y1,s);rectangle(50,50,450,450);
