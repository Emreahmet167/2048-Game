			if(d13==d14&&d14!=0){solx(d13,d14,400,100,10,e,15);d13+=d14,d14=0;d=1;if(d12==0&&d11==0&&d13>0){sol2x(d12,d13,300,100,10,e,15);d11=d13,d11=0;};if(d12==0&&d11!=0&&d13>0){solx(d12,d13,300,100,10,e,15);d12=d13,d13=0;};}
			if(d12==d13&&d13!=0){solx(d12,d13,300,100,10,e,15);d12+=d13,d13=0;d=1;if(d11==0&&d12>0){solx(d11,d12,200,100,10,e,15);d11=d12,d12=0;};}
			if(d11==d12&&d12!=0){solx(d11,d12,200,100,10,e,15);d11+=d12,d12=0;d=1;}
			if(d23==d21&&d21!=0&&d22==0){sol2x(d21,d23,300,200,10,e,15);d21+=d23,d23=0;d=1;};if(d22==d21&&d21!=0){solx(d21,d22,200,200,10,e,15);d21+=d22,d22=0;d=1;};if(d23==d22&&d22!=0){solx(d22,d23,300,200,10,e,15);d22+=d23,d23=0;d=1;};if(d24==d23&&d23!=0){solx(d23,d24,400,200,10,e,15);d23+=d24,d24=0;d=1;};//*o*nc*u*l toplama, sonradan eklendi
			if(d23==d24&&d24!=0){solx(d23,d24,400,200,10,e,15);d23+=d24,d24=0;d=1;if(d22==0&&d21==0&&d23>0){sol2x(d22,d23,300,200,10,e,15);d21=d23,d21=0;};if(d22==0&&d21!=0&&d23>0){solx(d22,d23,300,200,10,e,15);d22=d23,d23=0;};}
			if(d22==d23&&d23!=0){solx(d22,d23,300,200,10,e,15);d22+=d23,d23=0;d=1;if(d21==0&&d22>0){solx(d21,d22,200,200,10,e,15);d21=d22,d22=0;};}
			if(d21==d22&&d22!=0){solx(d21,d22,200,200,10,e,15);d21+=d22,d22=0;d=1;}
			if(d33==d31&&d31!=0&&d32==0){sol2x(d31,d33,300,300,10,e,15);d31+=d33,d33=0;d=1;};if(d32==d31&&d31!=0){solx(d31,d32,200,300,10,e,15);d31+=d32,d32=0;d=1;};if(d33==d32&&d32!=0){solx(d32,d33,300,300,10,e,15);d32+=d33,d33=0;d=1;};if(d34==d33&&d33!=0){solx(d33,d34,400,300,10,e,15);d33+=d34,d34=0;d=1;};//*o*nc*u*l toplama, sonradan eklendi
			if(d33==d34&&d34!=0){solx(d33,d34,400,300,10,e,15);d33+=d34,d34=0;d=1;if(d32==0&&d31==0&&d33>0){sol2x(d32,d33,300,300,10,e,15);d31=d33,d31=0;};if(d32==0&&d31!=0&&d33>0){solx(d32,d33,300,300,10,e,15);d32=d33,d33=0;};}
			if(d32==d33&&d33!=0){solx(d32,d33,300,300,10,e,15);d32+=d33,d33=0;d=1;if(d31==0&&d32>0){solx(d31,d32,200,300,10,e,15);d31=d32,d32=0;};}
			if(d31==d32&&d32!=0){solx(d31,d32,200,300,10,e,15);d31+=d32,d32=0;d=1;}
			if(d43==d41&&d41!=0&&d42==0){sol2x(d41,d43,300,400,10,e,15);d41+=d43,d43=0;d=1;};if(d42==d41&&d41!=0){solx(d41,d42,200,400,10,e,15);d41+=d42,d42=0;d=1;};if(d43==d42&&d42!=0){solx(d42,d43,300,400,10,e,15);d42+=d43,d43=0;d=1;};if(d44==d43&&d43!=0){solx(d43,d44,400,400,10,e,15);d43+=d44,d44=0;d=1;};//*o*nc*u*l toplama, sonradan eklendi
			if(d43==d44&&d44!=0){solx(d43,d44,400,400,10,e,15);d43+=d44,d44=0;d=1;if(d42==0&&d41==0&&d43>0){sol2x(d42,d43,300,400,10,e,15);d41=d43,d41=0;};if(d42==0&&d41!=0&&d43>0){solx(d42,d43,300,400,10,e,15);d42=d43,d43=0;};}
			if(d42==d43&&d43!=0){solx(d42,d43,300,400,10,e,15);d42+=d43,d43=0;d=1;if(d41==0&&d42>0){solx(d41,d42,200,400,10,e,15);d41=d42,d42=0;};}
			if(d41==d42&&d42!=0){solx(d41,d42,200,400,10,e,15);d41+=d42,d42=0;d=1;}
			//aralarÃ½nda bosluk olan sayilari topla
			if(d11==d14&&d11!=0&&d12==0&&d13==0){sol3x(d11,d14,400,100,10,e,15);d11+=d14,d14=0;d=1;};if(d21==d24&&d21!=0&&d22==0&&d23==0){sol3x(d21,d24,400,200,10,e,15);d21+=d24,d24=0;d=1;};if(d31==d34&&d31!=0&&d32==0&&d33==0){sol3x(d31,d34,400,300,10,e,15);d31+=d34,d34=0;d=1;};if(d41==d44&&d41!=0&&d42==0&&d43==0){sol3x(d41,d44,400,400,10,e,15);d41+=d44,d44=0;d=1;};//1-4
			if(d11==d13&&d11!=0&&d12==0){sol2x(d11,d13,300,100,10,e,15);d11+=d13,d13=0;d=1;};if(d21==d23&&d21!=0&&d22==0){sol2x(d21,d23,300,200,10,e,15);d21+=d23,d23=0;d=1;};if(d31==d33&&d31!=0&&d32==0){sol2x(d31,d33,300,300,10,e,15);d31+=d33,d33=0;d=1;};if(d41==d43&&d41!=0&&d42==0){sol2x(d41,d43,300,400,10,e,15);d41+=d43,d43=0;d=1;};//1-3
			if(d12==d14&&d12!=0&&d13==0){sol2x(d12,d14,400,100,10,e,15);d12+=d14,d14=0;d=1;if(d11==0){solx(d11,d12,200,100,10,e,15);d11=d12,d12=0;};};if(d22==d24&&d22!=0&&d23==0){sol2x(d22,d24,400,200,10,e,15);d22+=d24,d24=0;d=1;if(d21==0){solx(d21,d22,200,200,10,e,15);d21=d22,d22=0;};};//2-4
			if(d32==d34&&d32!=0&&d33==0){sol2x(d32,d34,400,300,10,e,15);d32+=d34,d34=0;d=1;if(d11==0){solx(d31,d32,200,300,10,e,15);d31=d32,d32=0;};};if(d42==d44&&d42!=0&&d43==0){sol2x(d42,d44,400,400,10,e,15);d42+=d44,d44=0;d=1;if(d41==0){solx(d41,d42,200,400,10,e,15);d41=d42,d42=0;};};
			//tekli sayilari ustteki bosluklara aktar
			if(d14>0){if(d11==0&&d12==0&&d13==0){sol3x(d11,d14,400,100,10,e,15);d11=d14,d14=0;d=1;};if(d14>0&&d11!=0&&d12==0&&d13==0){sol2x(d12,d14,400,100,10,e,15);d12=d14,d14=0;d=1;};if(d14>0&&d11!=0&&d12!=0&&d13==0){solx(d13,d14,400,100,10,e,15);d13=d14,d14=0;d=1;};}
			if(d13>0){if(d11==0&&d12==0){sol2x(d11,d13,300,100,10,e,15);d11=d13,d13=0;d=1;};if(d13>0&&d11!=0&&d12==0){solx(d12,d13,300,100,10,e,15);d12=d13,d13=0;d=1;if(d14>0&&d13==0){solx(d13,d14,400,100,10,e,15);d13=d14,d14=0;};};}
			if(d12>0){if(d11==0){solx(d11,d12,200,200,10,e,15);d11=d12,d12=0;d=1;};}
			if(d24>0){if(d21==0&&d22==0&&d23==0){sol3x(d21,d24,400,200,10,e,15);d21=d24,d24=0;d=1;};if(d24>0&&d21!=0&&d22==0&&d23==0){sol2x(d22,d24,400,200,10,e,15);d22=d24,d24=0;d=1;};if(d24>0&&d21!=0&&d22!=0&&d23==0){solx(d23,d24,400,200,10,e,15);d23=d24,d24=0;d=1;};}
			if(d23>0){if(d21==0&&d22==0){sol2x(d21,d23,300,200,10,e,15);d21=d23,d23=0;d=1;};if(d23>0&&d21!=0&&d22==0){solx(d22,d23,300,200,10,e,15);d22=d23,d23=0;d=1;if(d24>0&&d13==0){solx(d23,d24,400,200,10,e,15);d23=d24,d24=0;};};}
			if(d22>0){if(d21==0){solx(d21,d22,200,200,10,e,15);d21=d22,d22=0;d=1;};}
			if(d34>0){if(d31==0&&d32==0&&d33==0){sol3x(d31,d34,400,300,10,e,15);d31=d34,d34=0;d=1;};if(d34>0&&d31!=0&&d32==0&&d33==0){sol2x(d32,d34,400,300,10,e,15);d32=d34,d34=0;d=1;};if(d34>0&&d31!=0&&d32!=0&&d33==0){solx(d33,d34,400,300,10,e,15);d33=d34,d34=0;d=1;};}
			if(d33>0){if(d31==0&&d32==0){sol2x(d31,d33,300,300,10,e,15);d31=d33,d33=0;d=1;};if(d33>0&&d31!=0&&d32==0){solx(d32,d33,300,300,10,e,15);d32=d33,d33=0;d=1;if(d34>0&&d33==0){solx(d33,d34,400,300,10,e,15);d33=d34,d14=0;};};}
			if(d32>0){if(d31==0){solx(d31,d32,200,300,10,e,15);d31=d32,d32=0;d=1;};}
			if(d44>0){if(d41==0&&d42==0&&d43==0){sol3x(d41,d44,400,400,10,e,15);d41=d44,d44=0;d=1;};if(d44>0&&d41!=0&&d42==0&&d43==0){sol2x(d42,d44,400,400,10,e,15);d42=d44,d44=0;d=1;};if(d44>0&&d41!=0&&d42!=0&&d43==0){solx(d43,d44,400,400,10,e,15);d43=d44,d44=0;d=1;};}
			if(d43>0){if(d41==0&&d42==0){sol2x(d41,d43,300,400,10,e,15);d41=d43,d43=0;d=1;};if(d43>0&&d41!=0&&d42==0){solx(d42,d43,300,400,10,e,15);d42=d43,d43=0;d=1;if(d44>0&&d43==0){solx(d43,d44,400,400,10,e,15);d43=d44,d44=0;};};}
			if(d42>0){if(d41==0){solx(d41,d42,200,400,10,e,15);d41=d42,d42=0;d=1;};}
			
			if(d==0&&d11>0&&d12>0&&d13>0&&d14>0&&d21>0&&d22>0&&d23>0&&d24>0&&d31>0&&d32>0&&d33>0&&d34>0&&d41>0&&d42>0&&d43>0&&d44>0){
				//oyunun bitisi
			}
			
			b=rand();
			b=b%2;if(b==0){b=2;};
			if(b==1){
				while(1){
				c=getsayi();
				if(c==11&&d11==0){d11=2;break;};if(c==12&&d12==0){d12=2;break;};if(c==13&&d13==0){d13=2;break;};if(c==14&&d14==0){d14=2;break;};//bir sayi ekle
				if(c==21&&d21==0){d21=2;break;};if(c==22&&d22==0){d22=2;break;};if(c==23&&d23==0){d23=2;break;};if(c==24&&d24==0){d24=2;break;};
				if(c==31&&d31==0){d31=2;break;};if(c==32&&d32==0){d32=2;break;};if(c==33&&d33==0){d33=2;break;};if(c==34&&d34==0){d34=2;break;};
				if(c==41&&d41==0){d41=2;break;};if(c==42&&d42==0){d42=2;break;};if(c==43&&d43==0){d43=2;break;};if(c==44&&d44==0){d44=2;break;};
				}
				
			}
			if(b==2){
				while(1){
				c=getsayi();
				if(c==11&&d11==0){d11=2;break;};if(c==12&&d12==0){d12=2;break;};if(c==13&&d13==0){d13=2;break;};if(c==14&&d14==0){d14=2;break;};//iki sayi ekle
				if(c==21&&d21==0){d21=2;break;};if(c==22&&d22==0){d22=2;break;};if(c==23&&d23==0){d23=2;break;};if(c==24&&d24==0){d24=2;break;};
				if(c==31&&d31==0){d31=2;break;};if(c==32&&d32==0){d32=2;break;};if(c==33&&d33==0){d33=2;break;};if(c==34&&d34==0){d34=2;break;};
				if(c==41&&d41==0){d41=2;break;};if(c==42&&d42==0){d42=2;break;};if(c==43&&d43==0){d43=2;break;};if(c==44&&d44==0){d44=2;break;};
				}
				while(1){
				c=getsayi();
				if(c==11&&d11==0){d11=2;break;};if(c==12&&d12==0){d12=2;break;};if(c==13&&d13==0){d13=2;break;};if(c==14&&d14==0){d14=2;break;};
				if(c==21&&d21==0){d21=2;break;};if(c==22&&d22==0){d22=2;break;};if(c==23&&d23==0){d23=2;break;};if(c==24&&d24==0){d24=2;break;};
				if(c==31&&d31==0){d31=2;break;};if(c==32&&d32==0){d32=2;break;};if(c==33&&d33==0){d33=2;break;};if(c==34&&d34==0){d34=2;break;};
				if(c==41&&d41==0){d41=2;break;};if(c==42&&d42==0){d42=2;break;};if(c==43&&d43==0){d43=2;break;};if(c==44&&d44==0){d44=2;break;};
				if(d11>0&&d12>0&&d13>0&&d14>0&&d21>0&&d22>0&&d23>0&&d24>0&&d31>0&&d32>0&&d33>0&&d34>0&&d41>0&&d42>0&&d43>0&&d44>0){break;}
				}
			}
			d=0;
		sprintf(s11,"%d",d11),sprintf(s12,"%d",d12),sprintf(s13,"%d",d13),sprintf(s14,"%d",d14);
		sprintf(s21,"%d",d21),sprintf(s22,"%d",d22),sprintf(s23,"%d",d23),sprintf(s24,"%d",d24);
		sprintf(s31,"%d",d31),sprintf(s32,"%d",d32),sprintf(s33,"%d",d33),sprintf(s34,"%d",d34);
		sprintf(s41,"%d",d41),sprintf(s42,"%d",d42),sprintf(s43,"%d",d43),sprintf(s44,"%d",d44);
		settextstyle(1,HORIZ_DIR,3);
		outtextxy(100,100,s11),outtextxy(200,100,s12),outtextxy(300,100,s13),outtextxy(400,100,s14);
		outtextxy(100,200,s21),outtextxy(200,200,s22),outtextxy(300,200,s23),outtextxy(400,200,s24);
		outtextxy(100,300,s31),outtextxy(200,300,s32),outtextxy(300,300,s33),outtextxy(400,300,s34);
		outtextxy(100,400,s41),outtextxy(200,400,s42),outtextxy(300,400,s43),outtextxy(400,400,s44);
		}
	}
	getch();
	closegraph();
}

