int sol3x(int d1,int d2,int x1,int y1,int t1,int t2,int r){
	char s[20];
	int i=0;
	sprintf(s,"%d",d2);
	setfillstyle(1,r);
	for(i=0;i<300;i+=t1*3){//400
		while(i==0){setcolor(r);rectangle(x1,y1-i,x1+15,y1+30);floodfill(x1+1,y1-i+1,r);setcolor(0);outtextxy(x1,y1-i,s);delay(t2);break;};
		while(i==t1*3){setcolor(r);outtextxy(x1-i+30,y1,s);rectangle(x1-i,y1,x1+30,y1+25);floodfill(x1-i+1,y1+1,r);setcolor(0);outtextxy(x1-i,y1,s);delay(t2);break;};
		while(i==t1*6){setcolor(r);outtextxy(x1-i+30,y1,s);rectangle(x1-i,y1,x1+30,y1+25);floodfill(x1-i+1,y1+1,r);setcolor(0);outtextxy(x1-i,y1,s);delay(t2);break;};
		while(i==t1*9){setcolor(r);outtextxy(x1-i+30,y1,s);outtextxy(x1-i-10,y1,s);rectangle(x1-i,y1,x1+30,y1+25);floodfill(x1-i+1,y1+1,r);setcolor(0);outtextxy(x1-i,y1,s);rectangle(50,50,450,450);
