		while(i==t1*15){setcolor(r);outtextxy(x1-i+30,y1,s);rectangle(x1-i,y1,x1+30,y1+25);floodfill(x1-i+1,y1+1,r);setcolor(0);outtextxy(x1-i,y1,s);rectangle(50,50,450,450);
